<!-- emailTemplate.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title><%= messageObj.title %></title>
</head>
<body style="background-color: #f4f4f4;">
    <table style="margin-left:auto;margin-right:auto;width:100%;max-width:700px;background:#ffffff;padding-bottom: 150px;padding-top: 25px;">
        <thead width="100%">
            <tr width="100%">
                <th colspan="3" width="100%">
                    <img style="width: 100%;margin-bottom: 10px;" src="<%= messageObj.image_url %>" alt="" />
                </th>
            </tr>
        </thead>
        <tbody width="100%">
            <tr width="100%">
                <td width="5%">&nbsp;</td>
                <td width="90%">
                    <div style="margin-top: 30px;">
                        <%- messageObj.message_info %>
                    </div>
                </td>
                <td width="5%">&nbsp;</td>
            </tr>
        </tbody>
    </table>
</body>
</html>